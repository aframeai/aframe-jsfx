{
  "name": "aframe-jsfx",
  "version": "1.0.1",
  "description": "Use game sounds generated by loov-jsfx in A-Frame",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=dev parcel --out-dir build examples/index.html",
    "dist": "npm-run-all dist-js dist-min-js",
    "dist-js": "parcel build index.js --out-file aframe-jsfx.min.js",
    "dist-min-js": "parcel build index.js --no-minify --out-file aframe-jsfx.js"
  },
  "author": "aframeai",
  "license": "MIT",
  "dependencies": {
    "loov-jsfx": "^1.2.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "parcel": "1.12.4",
    "parcel-plugin-static-files-copy": "^2.5.0"
  },
  "staticFiles": {
    "staticPath": [
      {
        "env": "dev",
        "staticPath": "examples/sounds.json",
        "staticOutDir": "/"
      }
    ]
  }
}
